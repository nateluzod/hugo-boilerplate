{
  "name": "hugo-boilerplate",
  "version": "1.0.0",
  "description": "Starting point for Hugo projects",
  "main": "./static/bundle.js",
  "head": "./static/head.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && hugo",
    "subdir": "gulp subdir",
    "dev": "concurrently --kill-others \"hugo server --disableFastRender\" \"gulp\" \"cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nateluzod/hugo-boilerplate.git"
  },
  "keywords": [
    "hugo",
    "static",
    "boilerplate",
    "tailwind"
  ],
  "author": "Nate Luzod",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nateluzod/hugo-boilerplate/issues"
  },
  "homepage": "https://github.com/nateluzod/hugo-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "child_process": "^1.0.2",
    "concurrently": "^3.5.1",
    "core-js": "^2.5.7",
    "cross-env": "^5.1.5",
    "cssnano": "^3.10.0",
    "domready": "^1.0.8",
    "element-closest": "^2.0.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-inject": "^4.3.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-postcss": "^7.0.1",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^4.0.1",
    "gulp-string-replace": "^1.1.0",
    "gulp-svgmin": "^1.2.4",
    "gulp-svgstore": "^6.1.1",
    "gulp-uglifyjs": "^0.6.2",
    "gulp-uncss": "^1.0.6",
    "gulp-watch": "^4.3.11",
    "hamburgers": "^0.9.3",
    "hugo-bin": "^0.29.0",
    "jump.js": "^1.0.2",
    "laravel-mix": "^2.1.11",
    "laravel-mix-svg-sprite": "0.0.1",
    "laravel-mix-tailwind": "^0.1.0",
    "mutation-observer-inner-html-shim": "^1.0.0",
    "node-sass": "^4.9.0",
    "node-sass-magic-importer": "^5.1.2",
    "param-by-name": "^1.0.1",
    "path": "^0.12.7",
    "picturefill": "^3.0.2",
    "postcss": "^6.0.14",
    "postcss-nesting": "^5.0.0",
    "postcss-uncss": "^0.16.1",
    "postcss-unprefix": "^2.1.3",
    "run-sequence": "^2.2.1",
    "stimulus": "^1.0.1",
    "svg4everybody": "^2.1.9",
    "tailwindcss": "^0.4.0",
    "turbolinks": "^5.1.1",
    "uncss": "^0.16.2",
    "webpack": "^3.10.0",
    "yarn": "^1.3.2"
  }
}
