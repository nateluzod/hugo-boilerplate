{
  "name": "hugo-boilerplate",
  "version": "1.0.0",
  "description": "Starting point for Hugo projects",
  "main": "./static/bundle.js",
  "head": "./static/head.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "gulp build",
    "rollup": "rollup ./src/js/index.js --output.format iife --output.file ./static/bundle.js",
    "dev": "concurrently --kill-others \"hugo server --disableFastRender\" \"gulp\"",
    "index": "./node_modules/lunr-hugo/bin/index.js -i content -o ./static/lunr.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nateluzod/hugo-boilerplate.git"
  },
  "keywords": [
    "hugo",
    "static",
    "boilerplate",
    "tailwind"
  ],
  "author": "Nate Luzod",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nateluzod/hugo-boilerplate/issues"
  },
  "homepage": "https://github.com/nateluzod/hugo-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "child_process": "^1.0.2",
    "concurrently": "^3.5.1",
    "cssnano": "^3.10.0",
    "domready": "^1.0.8",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-inject": "^4.3.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-postcss": "^7.0.1",
    "gulp-svgmin": "^1.2.4",
    "gulp-svgstore": "^6.1.1",
    "gulp-uglifyjs": "^0.6.2",
    "gulp-uncss": "^1.0.6",
    "gulp-watch": "^4.3.11",
    "hugo-bin": "^0.18.1",
    "lunr-hugo": "^0.1.2",
    "path": "^0.12.7",
    "postcss": "^6.0.14",
    "rollup": "^0.55.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-uglify": "^3.0.0",
    "run-sequence": "^2.2.1",
    "tailwindcss": "^0.4.0",
    "turbolinks": "^5.1.1",
    "webpack": "^3.10.0",
    "yarn": "^1.3.2"
  }
}
